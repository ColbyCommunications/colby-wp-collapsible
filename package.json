{
  "name": "colby-wp-collapsible",
  "version": "1.0.12",
  "description": "A WordPress shortcode for collapsible HTML drawers.",
  "main": "dist/index.js",
  "scripts": {
    "start":
      "parcel example/src/index.html -d example -o --public-url /wp-collapsible/example/",
    "build":
      "parcel build src/js/index.js && postcss src/css/colby-wp-collapsible.css -e production -o dist/colby-wp-collapsible.css",
    "lint": "eslint src/js",
    "prepare": "npm build"
  },
  "author": "John Watkins",
  "keywords": ["wordpress"],
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.1.2",
    "babel-preset-env": "^1.6.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.14.0",
    "eslint-config-fbjs": "^2.0.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-relay": "^0.0.20",
    "node-autodeploy-wp": "^1.0.20",
    "parcel-bundler": "^1.4.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "postcss": "^6.0.15"
  }
}
